{
    "plugins": [
      [
        "module-resolver",
        {
          "root": ["."],
          "alias": {
            "styles": "scss/"
          },
          "cwd": "babelrc"
        }
      ],
      [
        "wrap-in-js",
        {
          "extensions": ["css$", "scss$"]
        }
      ]
    ],
    "presets": ["next/babel"],
    "ignore": [],
    "env": {
      "development": {
        "presets": ["next/babel"]
      },
      "production": {
        "presets": ["next/babel"]
      },
      "test": {
        "presets": [["next/babel", { "preset-env": { "modules": "commonjs" } }]]
      }
    }
  }